[Unit]
Description=Celery Worker
After=network.target

[Service]
User=username
Group=username
WorkingDirectory=/home/username/visualdynamics/apps/server
ExecStart=/home/username/miniconda3/envs/visualdynamics/bin/celery -A server.celery_tasks worker --concurrency=1
Restart=always
RestartSec=10

[Install]
WantedBy=multi-user.target